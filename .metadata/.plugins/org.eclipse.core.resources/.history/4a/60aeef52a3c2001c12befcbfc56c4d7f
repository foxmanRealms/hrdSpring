package kr.book.mapper;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.NonNull;
import lombok.ToString;

@Data
@AllArgsConstructor
@NoArgsConstructor
@ToString
public class BookVO {
	private int num; //  번호
	@NonNull
	private String title; //  제목
	@NonNull
	private String contents; //  작가
	private int count; //  출판사
	@NonNull
	private String writer; //  ISBN
	private String indate; //  보유도서
	
}
